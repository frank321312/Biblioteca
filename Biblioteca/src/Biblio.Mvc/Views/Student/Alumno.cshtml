@model IEnumerable<Biblio.Core.Alumno>

<header>
    <h1>Alumnos</h1>
</header>
<div class="table-responsive">
    <table class="table table-striped table-hover">
        <tr class="bg-dark text-white">
            <th>Dni</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Celular</th>
            <th>Email</th>
            <th>idCurso</th>
        </tr>
        @foreach (var alumno in Model)
        {
            <tr>
                <td>@alumno.Dni</td>
                <td>@alumno.Nombre</td>
                <td>@alumno.Apellido</td>
                <td>@alumno.Celular</td>
                <td>@alumno.Email</td>
                <td>@alumno.IdCurso</td>
            </tr>
        }
    </table>
</div>