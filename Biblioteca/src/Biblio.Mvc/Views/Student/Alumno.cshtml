@model IEnumerable<Biblio.Core.Alumno>

<header>
    <h1>Alumno</h1>
</header>
<div class="container-autor">
    <div class="fila">
        <ul class="cabecera">
            <li class="item">Dni</li>
            <li class="item">Nombre</li>
            <li class="item">Apellido</li>
            <li class="item">Celular</li>
            <li class="item">Email</li>
            <li class="item">idCurso</li>
            
        </ul>
    </div>
    @foreach (var item in Model)
    {
        <div class="fila">
            <ul class="lista">
                <li class="item">@item.Dni</li>
                <li class="item">@item.Nombre</li>
                <li class="item">@item.Apellido</li>
                <li class="item">@item.Celular</li>
                <li class="item">@item.Email</li>
                <li class="item">@item.IdCurso</li>
            </ul>
        </div>
    }
</div>
